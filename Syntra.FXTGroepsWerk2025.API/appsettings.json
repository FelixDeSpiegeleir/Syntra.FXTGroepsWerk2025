{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "JwtSettings": {
    "SecretKey": "your-very-long-secret-key-at-least-32-characters!", // Use a long secure key in real apps
    "Issuer": "MyApiIssuer",
    "Audience": "MyApiAudience",
    "ExpiryMinutes": 60
  }
}
